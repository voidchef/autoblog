{
  "name": "autoblog",
  "version": "1.0.0",
  "description": "AI written blog posts",
  "main": "dist/index.js",
  "repository": "https://github.com/voidchef/autoblog.git",
  "bugs": {
    "url": "https://github.com/voidchef/autoblog/issues"
  },
  "author": "Neelutpal Saha <neelutpalsaha44@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:build": "tsc --build && pm2 start ecosystem.config.json --no-daemon",
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --import=extensionless/register dist/index.js",
    "front": "cd front && pnpm dev",
    "server": "concurrently --kill-others \"pnpm compile:watch\" \"pnpm pre:dev\"",
    "dev": "concurrently \"pnpm server\" \"pnpm front\"",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles",
    "test:ts": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'ts$'",
    "test:js": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'js$'",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --watchAll",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage",
    "coverage:coveralls": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage --coverageReporters=lcov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky",
    "release": "standard-version && git push --follow-tags origin master",
    "commit": "git add -A && cz"
  },
  "keywords": [
    "autoblog"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.0.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/tmp": "^0.2.6",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "^9.2.0",
    "coveralls": "^3.1.1",
    "eslint": "^9.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-security": "^3.0.1",
    "extensionless": "^1.9.9",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@dqbd/tiktoken": "^1.0.21",
    "@google-analytics/data": "^5.2.0",
    "@google-cloud/text-to-speech": "^6.3.1",
    "@langchain/community": "0.3.49",
    "@langchain/core": "0.3.66",
    "@langchain/mistralai": "0.2.1",
    "@langchain/openai": "0.6.3",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "joi": "^17.13.3",
    "json5": "^2.2.3",
    "jsonschema": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.30",
    "moment": "^2.30.1",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "ora": "^8.2.0",
    "p-retry": "^6.2.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^6.0.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tmp": "^0.2.3",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
